/* pages/food/food.wxss */
@import'../../templates/productList/productList.wxss';
page{
  background:#f5f5f5;
}
.header{
  background: #fff;
  padding: 10rpx;
  display: flex;
}
.location{
  width: 100rpx;
  font-size: 28rpx;
  
}
.inp{
  flex:1;
}
.nav{
  background: #f5f5f5;
  font-size: 28rpx;
  color: #666;
  border-radius: 28rpx;
  width: 100%;
  text-align: center;
}
.navBar{
  background: #fff;
  display: flex;
  margin-top: 20rpx;
  flex-wrap: wrap;
}
.item{
  width: 25%;
  text-align: center;
  margin-bottom: 20rpx;
  color:#666;
}
.item image{
  width: 74rpx;
  height: 74rpx;
}